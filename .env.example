# WuzAPI Configuration
WUZAPI_PORT=8080
WUZAPI_ADMIN_TOKEN=your_admin_token_here_replace_with_strong_random_value
WUZAPI_GLOBAL_ENCRYPTION_KEY=32_byte_encryption_key_here_replace_with_strong_random_value
WUZAPI_GLOBAL_HMAC_KEY=global_hmac_key_here_replace_with_strong_random_value
WUZAPI_GLOBAL_WEBHOOK=https://yourdomain.com/webhook
WEBHOOK_FORMAT=json
SESSION_DEVICE_NAME=WuzAPI

# Database Configuration - These will be provided by EasyPanel
DB_USER=wuzapi
DB_PASSWORD=your_db_password
DB_NAME=wuzapi
DB_HOST=your_db_host
DB_PORT=5432
DB_SSLMODE=false

# Timezone
TZ=America/Sao_Paulo

# RabbitMQ (Optional)
RABBITMQ_URL=amqp://wuzapi:wuzapi@localhost:5672/%2F
RABBITMQ_QUEUE=whatsapp_events